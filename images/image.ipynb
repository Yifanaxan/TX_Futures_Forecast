    "from PIL import Image\n",
    "\n",
    "# open the image file\n",
    "image = Image.open(\"bullbear7.jpg\")\n",
    "\n",
    "# set the size of the resized image\n",
    "new_size = (918, 344)\n",
    "\n",
    "# resize the image\n",
    "resized_image = image.resize(new_size)\n",
    "\n",
    "# save the resized image to a new file\n",
    "resized_image.save(\"resized_bullbear7.jpg\")\n"
   ]
  }
